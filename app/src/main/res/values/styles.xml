<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorBlack</item>
        <item name="colorPrimaryDark">@color/colorPrimary</item>
        <item name="colorAccent">#32FA0F</item>
        <item name="android:statusBarColor">#000000</item>
    </style>

    <style name="ToolbarColoredBackArrow" parent="AppTheme">
        <item name="android:textColorSecondary">#32FA0F</item>
    </style>

    <style name="TextAppearance.AppCompat.Widget.ActionBar.Title" parent="@android:style/TextAppearance">

        <item name="android:textColor">@color/colorPrimary_1</item>

    </style>

    <style name="AppThemeActionBarStyle" parent="@style/Widget.AppCompat.Light.ActionBar">
        <item name="background">@color/colorPrimary_1</item>
    </style>



</resources>
